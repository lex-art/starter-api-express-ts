{
  "name": "api-express-ts",
  "version": "1.0.0",
  "description": "",
  "main": "build/index.js",
  "scripts": {
    "start": "node build/index.js",
    "build": "tsc",
    "dev": "ts-node-dev src/index.ts",
    "init-tsc": "tsc --init",
    "knex:init": "knex --cwd src/db init -x ts ",
    "knex:migrate:make": "knex --knexfile src/db/knexfile.ts migrate:make",
    "knex:migrate:latest": "knex --knexfile src/db/knexfile.ts migrate:latest",
    "knex:migrate:rollback": "knex --knexfile src/db/knexfile.ts migrate:rollback",
    "knex:migrate:up": "knex --knexfile src/db/knexfile.ts migrate:up",
    "knex:seed:make": "knex --knexfile src/db/knexfile.ts seed:make",
    "knex:seed:run": "knex --knexfile src/db/knexfile.ts seed:run"
  },
  "keywords": [],
  "author": "@lex-art",
  "license": "ISC",
  "devDependencies": {
    "@types/bcryptjs": "^2.4.2",
    "@types/cors": "^2.8.12",
    "@types/express": "^4.17.13",
    "@types/helmet": "^4.0.0",
    "@types/knex": "^0.16.1",
    "@types/morgan": "^1.9.3",
    "@typescript-eslint/eslint-plugin": "^5.22.0",
    "@typescript-eslint/parser": "^5.22.0",
    "eslint": "^8.14.0",
    "prettier": "^2.6.2",
    "standard": "^17.0.0",
    "ts-node": "^10.7.0",
    "ts-node-dev": "^1.1.8",
    "typescript": "^4.6.4"
  },
  "dependencies": {
    "bcryptjs": "^2.4.3",
    "cors": "^2.8.5",
    "dotenv": "^16.0.0",
    "express": "^4.18.1",
    "helmet": "^5.0.2",
    "knex": "^2.0.0",
    "morgan": "^1.10.0",
    "pg": "^8.7.3"
  },
  "exclude": [
    "**/.*",
    "node_modules",
    "typings"
  ]
}
